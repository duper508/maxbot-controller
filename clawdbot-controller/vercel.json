{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "apps/web/src/pages/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "apps/web"
    }
  ],
  "env": {
    "NEXT_PUBLIC_DISCORD_WEBHOOK_BASE": "@discord_webhook_base"
  },
  "buildCommand": "npm run build -- --filter=maxbot-controller-web",
  "outputDirectory": "apps/web/.next"
}
